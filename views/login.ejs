<%- include('partials/_header') %>
<body>
    <div class="title">
        <h1>Login Page</h1>
    </div>
    <form id="request" action="users/login" method="POST">
      <div class="form-group">
          <label class="h4" for="exampleInputUsername">Username</label>
          <input type="text" class="form-control" name="username" id="exampleInputUsername" placeholder="Username">
        </div>
      <div class="form-group">
        <label class="h4" for="exampleInputPassword1">Password</label>
        <input type="password" class="form-control" name="password" id="exampleInputPassword1" placeholder="Password">
      </div>
      <button type="submit" class="btn btn-warning">Log in</button>

    </form>
</body>
</html>